<p id="notice"><%= notice %></p>

<h1>Listing Api Listings</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_listings.each do |api_listing| %>
      <tr>
        <td><%= link_to 'Show', api_listing %></td>
        <td><%= link_to 'Edit', edit_api_listing_path(api_listing) %></td>
        <td><%= link_to 'Destroy', api_listing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Listing', new_api_listing_path %>
